<h1>Edit charity</h1>
<%= form_for @charity do |f| %>
  <div class="field">
    <%= f.label :name%>
    <%= f.text_field :name, required:true %>        
  </div>
  <div class="field">
     <%= f.label :description%>
    <%= f.text_field :description %>        
  </div>
  
  <%= f.submit %>
<% end %>
