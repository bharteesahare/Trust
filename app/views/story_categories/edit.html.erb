<h3>Add a Edit StoryCategory</h3>
<%= form_for @story_category do |f| %>
  <div class="form-group">
    <%= f.label :project_id, "Select Project"%>
   	<%= f.select(:project_id, Project.all.map{|r| [r.name, r.id]}, {}, { :class => 'form-control' }) %>          
  </div>
  	
  <div class="form-group">
    <%= f.label :name%>
    <%= f.text_field :name, required:true %>        
  </div>
  
  
  <%= f.submit %>
<% end %>
