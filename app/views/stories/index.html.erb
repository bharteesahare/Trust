<div class="container-fluid" style="margin: 30px !important;">
	<table style="border-collapse: collapse;width: 100%;border: 1px solid black;">
		<tr style=" border: 1px solid black;height: 50px;">
			<th style="border: 1px solid black;">Name</th>
			<th style="border: 1px solid black;">Project Name</th>
			<th style="border: 1px solid black;">Story Category Name</th>
		</tr>
		<% @stories.each do |story|%>
		<tr style="border: 1px solid black;">
			<td style="border: 1px solid black;"><%= story.name %></td>
			<td style="border: 1px solid black;"><%= story.project.name %></td>
			<td style="border: 1px solid black;"><%= story.story_category.name %></td>
		</tr>
		<% end %>
	</table>
</div>