<h3>Add a Edit Project</h3>
<%= form_for @project do |f| %>
  <div class="form-group">
    <%= f.label :charity_id, "Select Charity"%>
   	<%= f.select(:charity_id, Charity.all.map{|r| [r.name, r.id]}, {}, { :class => 'form-control' }) %>          
  </div>
  	
  <div class="form-group">
    <%= f.label :name%>
    <%= f.text_field :name, required:true %>        
  </div>
  
  <%= f.submit %>
<% end %>
